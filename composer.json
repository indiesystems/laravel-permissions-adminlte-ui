{
    "name": "indiesystems/laravel-permissions-adminlte-ui",
    "description": "A Laravel adminLTE bootstrap 4 UI package for spatie permissions",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/indiesystems/laravel-permissions-adminlte-ui",
    "support": {
        "issues": "https://github.com/indiesystems/laravel-permissions-adminlte-ui/issues",
        "source": "https://github.com/indiesystems/laravel-permissions-adminlte-ui"
    },
    "authors": [
        {
            "name": "Chris Dimas",
            "email": "chris@indie.systems",
            "homepage": "https://indie.systems",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "IndieSystems\\PermissionsAdminlteUi\\": "src/"
        },
        "files":[
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
